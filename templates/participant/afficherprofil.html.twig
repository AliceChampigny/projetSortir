{% extends 'base.html.twig' %}
{% block title %}Profil de {{ participant.pseudo }}{% endblock %}
{% block body %}
<h1>Profil de {{ participant.pseudo}}</h1>
<div class="container">
    <ul>
        <li>{{ participant.prenom}}</li>
        <li>{{ participant.nom }}</li>
        <li>{{ participant.telephone }}</li>
        <li>{{ participant.email }}</li>
        <li>{{ participant.campus.nom }}</li>
    </ul>

    <a href="{{ path('sortie_liste') }}"><button>Retour</button></a>
</div>
{% endblock %}



